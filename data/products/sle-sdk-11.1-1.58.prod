  <installconfig>
    <defaultlang>en_US</defaultlang>
    <datadir>suse</datadir>
    <descriptiondir>suse/setup/descr</descriptiondir>
    <releasepackage name="sle-sdk-release" flag="EQ" version="11.1" release="1.58" />
    <distribution>SUSE_SDK</distribution>
  </installconfig>
  <runtimeconfig />
