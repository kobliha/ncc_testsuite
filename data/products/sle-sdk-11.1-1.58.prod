  <summary>SUSE Linux Enterprise Software Development Kit 11 SP1</summary>
  <description>This is the SUSE Linux Enterprise Software Development Kit</description>
  <urls>
    <url name="bugtracker">https://bugzilla.novell.com/</url>
  </urls>
  <buildconfig>
    <producttheme>SLES</producttheme>
